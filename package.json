{
  "name": "material-angular-io",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "npm run build-themes && ng serve",
    "start-jit": "npm run build-themes && ng serve --aot=false",
    "lint": "tslint -p src/tsconfig.json",
    "test": "npm run build-themes && ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "fetch-local": "bash ./tools/fetch-assets-local.sh",
    "build-themes": "bash ./tools/build-themes.sh",
    "prod-build": "npm run build-themes && ng build --aot --prod",
    "postinstall": "webdriver-manager update && bash ./tools/build-themes.sh",
    "publish-prod": "bash ./tools/deploy.sh stable prod",
    "publish-dev": "bash ./tools/deploy.sh",
    "publish-beta": "bash ./tools/deploy.sh stable beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/material": "^7.2.1",
    "@angular/material-moment-adapter": "^7.2.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.6",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "firebase-tools": "^6.0.0",
    "highlight.js": "^9.9.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sauce-launcher": "^1.2.0",
    "natives": "^1.1.6",
    "node-sass": "^4.5.3",
